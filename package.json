{
  "name": "GraphQL-Authentication",
  "version": "1.0.0",
  "scripts": {
    "serve": "node 'dist/server.js'",
    "build": "tsc -p . && ncp src/schema dist/schema",
    "start:dev": "yarn run build:dev",
    "build:dev": "nodemon 'src/server.ts' --exec 'ts-node' src/server.ts -e ts,graphql"
  },
  "main": "index.js",
  "repository": "https://github.com/larson-carter/GraphQL-Authentication.git",
  "author": "Larson Carter <larsoncarter@blazermail.net>",
  "license": "MIT",
  "dependencies": {
    "apollo-server-express": "^2.17.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "graphql": "^15.3.0",
    "http": "0.0.1-security",
    "ncp": "^2.0.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.8",
    "@types/graphql": "^14.5.0",
    "@types/graphql-depth-limit": "^1.1.2",
    "@types/node": "^14.6.2",
    "graphql-depth-limit": "^1.1.0",
    "graphql-import": "^1.0.2",
    "graphql-import-node": "0.0.4",
    "nodemon": "^2.0.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  }
}
